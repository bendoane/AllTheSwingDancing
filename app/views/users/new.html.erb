
<div class="container">
  <h1>Welcome to Swing Out</h1>
  <h4>Let's get you signed up!</h4>
  <br>
  <div class="col-xs-6">
    <%=bootstrap_form_for(@user) do |f|%>
      <div class="field">
        <%= f.text_field :username%>
      </div>
      <br>
      <div class="field">
        <%= f.text_field :email%>
      </div>
      <br>
      <div class="field">
        <%= f.password_field :password%>
      </div>
      <br>
      <div class="field">
        <%= f.password_field :password_confirmation%>
      </div>
      <br>
      <%= f.submit%>
    <%end%>
  </div>
</div>
