<!--Facebook login SDK info  -->
<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '1667925190116529',
      xfbml      : true,
      version    : 'v2.5'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>
<!--  -->





<div class="container">
  <h1>Sign in!</h1>
  <div class="col-xs-6">
    <%=bootstrap_form_for(@user_session) do |f|%>
      <div class="field">
        <%= f.text_field :username, placeholder: "Frankie_Example"%>
      </div>
      <div class="field">
        <%= f.password_field :password, placeholder: "password"%>
      </div>
      <%= f.submit "Sign In!", class:"btn btn-primary btn-md"%>
    <%end%>
    <br>
    New to Swing Out?
    <%=button_to "Sign up now!", new_user_path, class: "btn btn-default btn-sm"%>
  </div>
</div>
