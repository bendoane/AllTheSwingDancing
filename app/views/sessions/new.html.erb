<div class="container">
  <h1>Sign in!</h1>
  <div class="col-xs-6">
    <%=bootstrap_form_for(@user_session) do |f|%>
      <div class="field">
        <%= f.text_field :username, placeholder: "Frankie_Example"%>
      </div>
      <div class="field">
        <%= f.password_field :password, placeholder: "password"%>
      </div>
      <br>
      <%= f.submit%>
    <%end%>
  </div>
</div>
